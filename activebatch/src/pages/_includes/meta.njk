{% if faqs %}
<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": [
        {% for q, a in faqs %}
                {
                    "@type": "Question",
                    "name": "{{ q | safe }}",
                    "acceptedAnswer": {
                        "@type": "Answer",
                        "text": "{{ a | escape }}"
                    }
                }{{ "," if not loop.last  }}
            {% endfor %}
        ]
    }
</script>
{% endif %}
